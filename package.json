{
  "name": "reactandexpressapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm public/*.map public/main.js public/main.css",
    "client": "esbuild --bundle --minify --target=es6 --outdir=client/public client/src/Main.jsx",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "client:dev": "esbuild --watch --bundle --sourcemap --target=es6 --outdir=public client/main.jsx",
    "server:dev": "nodemon --watch server dev",
    "server:prod": "node api/server.js prod"
  },
